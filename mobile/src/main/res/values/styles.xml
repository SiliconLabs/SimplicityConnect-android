<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="MainAppTheme" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <item name="android:windowBackground">@color/gallery</item>

        <item name="colorPrimary">@color/blue_primary</item>
        <item name="colorOnPrimary">@android:color/white</item>
        <item name="colorPrimaryDark">@color/blue_primary</item>
        <item name="colorSecondary">@color/blue_primary</item>

        <item name="bottomNavigationStyle">@style/BottomNavigationView</item>
        <item name="extendedFloatingActionButtonStyle">@style/ExtendedFloatingActionButton</item>
        <item name="android:alertDialogTheme">@style/AlertDialogTheme</item>
    </style>

    <style name="CustomSplashTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/si_connect_app_screen_background</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>


    </style>

    <style name="FullscreenTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowBackground">@color/gallery</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>


    </style>

    <!-- override system splash screen with just a matching color background -->
    <style name="SystemSplashTheme" parent="Theme.SplashScreen">
        <item name="android:windowBackground">@drawable/si_connect_app_screen_background</item>
        <item name="postSplashScreenTheme">@style/CustomSplashTheme</item>


    </style>


